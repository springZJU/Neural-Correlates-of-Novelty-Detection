{
    "_schemaVersion": "1.0.0",

    "mSave": {
        "inputs": [
            {"name": "FILENAME", "kind": "required", "type": ["file=*.mat"]},
            {"name": "variable", "kind": "required", "type": ["@(x) ~contains(x, '-')"], "repeating": true},
            {"name": "option", "kind": "ordered", "type": ["@(x) contains(x, '-')"], "repeating": true}
        ],
        "description": "已存在则不保存"
    },

    "mPrint": {
        "inputs": [
            {"name": "target", "kind": "required", "type": ["@(x) isgraphics(x)"]},
            {"name": "FILENAME", "kind": "required", "type": ["file=*.jpg, *.png, *.tiff, *.meta"]},
            {"name": "formattype", "kind": "ordered", "type": ["choices={'-djpeg', '-dpng', '-dtiff', '-dtiffn', '-dmeta'}"]},
            {"name": "resolution", "kind": "ordered", "type": ["string", "choices={'-r600', '-r300', '-r0'}"]}
        ],
        "description": "已存在则不保存"
    },

    "mLoad": {
        "inputs": [
            {"name": "FILENAME", "kind": "required", "type": [["file=*.mat"], ["@(arg) isempty(arg)"]]},
            {"name": "variables", "kind": "ordered", "type": [["cell"], ["string"], ["char"]]}
        ],
        "description": "已存在则不加载"
    },

    "loadDailyData": {
        "inputs": [
            {"name": "ROOTPATH", "kind": "required", "type": ["@(x) isstring(x)"]},
            {"name": "MATNAME", "kind": "namevalue", "type": ["@(x) isstring(x)"]},
            {"name": "DATE", "kind": "namevalue", "type": ["@(x) isstring(x)"]},
            {"name": "protocols", "kind": "namevalue", "type": ["@(x) isstring(x)"]},
            {"name": "all_or", "kind": "namevalue", "type": ["@(x) isstring(x)"]}
        ],
        "description": "将特定路径下的同名MAT文件整合"
    },

    "dirItem": {
        "inputs": [
            {"name": "rootPath", "kind": "required", "type": ["folder"]},
            {"name": "keyword", "kind": "required", "type": ["string"]},
            {"name": "folderOrFile", "kind": "namevalue", "type": ["choices = {'all', 'file', 'folder'}"]}
        ],
        "description": "返回跟目录下包含特定字符串的路径/文件"
    },

    "renameItem": {
        "inputs": [
            {"name": "rootPath", "kind": "required", "type": ["folder"]},
            {"name": "keyword", "kind": "required", "type": ["string"]},
            {"name": "newName", "kind": "required", "type": ["string"]},
            {"name": "folderOrFile", "kind": "namevalue", "type": ["choices = {'all', 'file', 'folder'}"]}
        ],
        "description": "重命名跟目录下包含特定字符串的路径/文件"
    },

    "pathManager": {
        "inputs": [
            {"name": "ROOTPATH", "kind": "required", "type": ["folder"]},
            {"name": "subjects", "kind": "namevalue", "type": [["cell"], ["string"], ["char"]]},
            {"name": "protocols", "kind": "namevalue", "type": [["cell"], ["string"], ["char"]]},
            {"name": "matPat", "kind": "namevalue", "type": [["string"], ["char"]]},
            {"name": "folderOnly", "kind": "namevalue", "type": ["logical", "scalar"]}
        ]
    }
}